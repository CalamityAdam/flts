{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "valid-jsdoc": "off",
    "indent": "off",
    "object-curly-spacing": "off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "off"
  }
}
